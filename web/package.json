{
  "name": "web",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "ng-openapi-gen": "ng-openapi-gen",
    "codegen": "npm run codegen-factorio-mod-portal && npm run codegen-mgmt-server-rest",
    "codegen-factorio-mod-portal": "npm run ng-openapi-gen -- -c openapi-factorio-mod-portal.json",
